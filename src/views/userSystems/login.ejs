<h3>Welcome Back, Login Please!</h3>



  <% if (messages.error) { %>
    <h6 id="err-msg"><b><%= messages.error %></b></h2>
  <% } %>


  <form action="login" method="POST">
    <input type="text" name="username" required="true" placeholder="Username">
    <br>
    <br>
    <input type="password" name="password" required="true" placeholder="Password">
    <br>
    <br>
    <br>
    <button type="submit">Login</button>
  </form>
  <br>
  <br>
  <p>
    Don't Have An Account? Sign Up
    <a href="http://localhost:3000/userSystems/register" style="text-decoration: none;">Here</a>
    !
  </p>
